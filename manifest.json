{
  "manifest_version": 3,
  "name": "ApvBlocker",
  "version": "0.1.1",
  "description": "Enhance your YouTube experience",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://youtube.com/*",
        "*://www.youtube.com/*",
        "*://m.youtube.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "override.css"
      ]
    }
  ]
}